<VirtualHost *:80>
    Header set Access-Control-Allow-Origin "*"
    ProxyRequests Off
    ProxyPreserveHost on
    ProxyPass / http://192.168.33.111:8080/
    ProxyPassReverse / http://192.168.33.111:8080/
    <Location />
        Order allow,deny
        Allow from all
    </Location>

</VirtualHost>
